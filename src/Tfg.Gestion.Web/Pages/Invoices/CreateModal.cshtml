@page
@using Tfg.Gestion.Localization
@using Tfg.Gestion.Web.Pages.Invoices
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<GestionResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Invoice" asp-page="/Invoices/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewInvoice"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
